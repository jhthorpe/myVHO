PROGRAM bleh
  INTEGER, DIMESION(:), ALLOCATABLE :: X,Y
  INTEGER :: n

  
  READ(100,*) n
  ALLOCATE(X(1:n))
  CALL equilv(n,X,Y)

CONTAINS


SUBROUTINE equilv(N,X,Y)
  IMPLICIT NONE
  INTEGER, DIMENSION(1:N), INTENT(IN) :: X
  INTEGER, DIMENSION(:), ALLOCATABLE, INTENT(INOUT) :: Y
  INTEGER, INTENT(IN) :: N
  
  ALLOCATE(Y(1:N))
END SUBROUTINE 


END PROGRAM
